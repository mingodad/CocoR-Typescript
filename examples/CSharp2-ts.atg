static readonly  maxTerminals : int = 160;  // set size

static  NewSet(params int[] values) : BitArray {
  let a : BitArray = new BitArray(this.maxTerminals);
  for (let x of values) a[x] = true;
  return a;
}

static unaryOp : BitArray      = this.NewSet(this._plus, this._minus, this._not, this._tilde, this._inc, this._dec, this._true, this._false),
  typeKW : BitArray       = this.NewSet(this._char, this._bool, this._object, this._string, this._sbyte, this._byte, this._short,

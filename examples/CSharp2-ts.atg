                 this._true, this._try, this._typeof, this._uint, this._ulong, this._unchecked, this._unsafe,
                 this._ushort, this._usingKW, this._virtual, this._void, this._volatile, this._while
                 ),
  typArgLstFol : BitArray = this.NewSet ( this._lpar, this._rpar, this._rbrack, this._colon, this._scolon, this._comma, this._dot,

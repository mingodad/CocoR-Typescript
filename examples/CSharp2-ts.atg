  unaryHead : BitArray    = this.NewSet(this._plus, this._minus, this._not, this._tilde, this._times, this._inc, this._dec, this._and),
  assnStartOp : BitArray  = this.NewSet(this._plus, this._minus, this._not, this._tilde, this._times),
  castFollower : BitArray = this.NewSet(this._tilde, this._not, this._lpar, this._ident,
                 /* literals */
                 this._intCon, this._realCon, this._charCon, this._stringCon,
                 /* any keyword expect as and is */
                 this._abstract, this._base, this._bool, this._break, this._byte, this._case, this._catch,
                 this._char, this._checked, this._class, this._const, this._continue, this._decimal, this._default,
                 this._delegate, this._do, this._double, this._else, this._enum, this._event, this._explicit,
                 this._extern, this._false, this._finally, this._fixed, this._float, this._for, this._foreach,
                 this._goto, this._if, this._implicit, this._in, this._int, this._interface, this._internal,
                 this._lock, this._long, this._namespace, this._new, this._null, this._object, this._operator,
                 this._out, this._override, this._params, this._private, this._protected, this._public,
                 this._readonly, this._ref, this._return, this._sbyte, this._sealed, this._short, this._sizeof,
